{
  "name": "punto-cambio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "nodemon --watch server --ext ts,tsx,json --exec \"tsx server/index.ts\"",
    "build": "npm run build:frontend && npm run build:server",
    "build:frontend": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "seed": "npx tsx prisma/seed.ts",
    "seed:admin": "npx tsx prisma/seed-admin.ts",
    "seed:complete": "npx tsx prisma/seed-complete.ts",
    "seed:production": "npx tsx prisma/seed-production.ts",
    "add:point": "npx tsx prisma/add-point.ts",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf dist server-dist node_modules/.cache",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "start:server": "node server-dist/index.js",
    "deploy": "git pull origin main && npm install && npm run build && npx prisma generate && npx prisma db push && pm2 start ecosystem.config.js --env production",
    "deploy:quick": "git pull origin main && npm run build && pm2 reload punto-cambio-api",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop punto-cambio-api",
    "pm2:restart": "pm2 reload punto-cambio-api",
    "pm2:logs": "pm2 logs punto-cambio-api",
    "pm2:status": "pm2 status",
    "migrate:agencias": "npx prisma db push"
  },
  "prisma": {
    "seed": "tsx prisma/seed-complete.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
