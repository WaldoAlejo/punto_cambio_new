{
  "name": "punto-cambio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "nodemon --watch server --ext ts,tsx,json --exec \"tsx server/index.ts\"",
    "build": "npm run build:frontend && npm run build:server",
    "build:frontend": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "seed:clean:dry": "tsx prisma/seed-clean-db.ts",
    "seed:clean": "tsx prisma/seed-clean-db.ts --execute",
    "seed": "npx tsx prisma/seed.ts",
    "seed:admin": "npx tsx prisma/seed-admin.ts",
    "seed:complete": "npx tsx prisma/seed-complete.ts",
    "seed:production": "npx tsx prisma/seed-production.ts",
    "prisma:clear-migrations": "npx tsx scripts/clear-migrations.ts",
    "migrate:fresh": "npm run prisma:clear-migrations && npx prisma migrate dev --name init",
    "project:clean:sh": "sh ./scripts/clean-project.sh",
    "project:clean:ps1": "pwsh ./scripts/clean-project.ps1",
    "project:clean": "node -e \"const os=require('os'); const {execSync}=require('child_process'); if(os.platform()==='win32'){ execSync('pwsh ./scripts/clean-project.ps1',{stdio:'inherit'}); } else { execSync('sh ./scripts/clean-project.sh',{stdio:'inherit'}); }\"",
    "add:point": "npx tsx prisma/add-point.ts",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf dist server-dist node_modules/.cache",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "start:server": "node server-dist/index.js",
    "deploy": "git pull origin main && npm install && npm run build && npx prisma generate && npx prisma db push && pm2 start ecosystem.config.cjs --env production",
    "deploy:quick": "git pull origin main && npm run build && pm2 reload punto-cambio-api",
    "pm2:start": "pm2 start ecosystem.config.cjs --env production",
    "pm2:stop": "pm2 stop punto-cambio-api",
    "pm2:restart": "pm2 reload punto-cambio-api",
    "pm2:logs": "pm2 logs punto-cambio-api",
    "pm2:status": "pm2 status",
    "migrate:agencias": "npx prisma db push",
    "transfer:report": "npx tsx server/scripts/report-transfer-issues.ts",
    "transfer:fix": "npx tsx server/scripts/fix-historical-transfers.ts",
    "transfer:menu": "npx tsx server/scripts/transfer-fix-menu.ts",
    "script:verificar-limpieza": "npx tsx server/scripts/verificar-antes-limpiar.ts",
    "script:limpiar-transacciones": "npx tsx server/scripts/limpiar-transacciones.ts",
    "clean:db": "npx tsx scripts/clean-database.ts",
    "clean:db:safe": "npx tsx scripts/clean-database-safe.ts",
    "audit:saldos": "npx tsx scripts/audit-saldos-inconsistentes.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed-complete.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
